<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/icons"
    tools:context="clh.inspecciones.com.inspecciones_v2.Fragments.AltaNuevaFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">


        <LinearLayout
            android:orientation="vertical"
            android:layout_gravity="center_horizontal"
            android:background="@color/colorPrimary"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/barra1"
                android:background="@color/divider"
                android:layout_width="match_parent"
                android:layout_height="3dp" />

            <TextView
                android:id="@+id/tvtipovehiculo"
                android:paddingTop="10dp"
                android:paddingBottom="6dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:gravity="center_horizontal"
                android:textSize="20sp"
                android:textStyle="bold"
                android:textColor="@color/icons"
                android:text="@string/tvTipoVehiculo" />

            <CheckBox
                android:id="@+id/cbrigido"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:textSize="16sp"
                android:textColor="@color/icons"
                android:text="@string/cbRigido" />

            <CheckBox
                android:id="@+id/cbconjunto"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:textSize="16sp"
                android:textColor="@color/icons"
                android:text="@string/cbSLO" />

            <CheckBox
                android:id="@+id/cbtractora"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:textSize="16sp"
                android:textColor="@color/icons"
                android:text="@string/cbTractora" />

            <CheckBox
                android:id="@+id/cbcisterna"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:textSize="16sp"
                android:textColor="@color/icons"
                android:text="@string/cbCisterna" />

            <ImageView
                android:id="@+id/barra"
                android:background="@color/colorSecondaryText"
                android:layout_width="match_parent"
                android:layout_height="3dp" />

        </LinearLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:background="@color/colorPrimary"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/tvtipoinspeccion"
            android:paddingTop="10dp"
            android:paddingBottom="6dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:gravity="center_horizontal"
            android:textSize="20sp"
            android:textStyle="bold"
            android:textColor="@color/icons"
            android:text="@string/tvTipoInspeccion" />

        <CheckBox
            android:id="@+id/cbligeros"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:textSize="16sp"
            android:textColor="@color/icons"
            android:text="@string/cbligeros" />

        <CheckBox
            android:id="@+id/cbpesados"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:textSize="16sp"
            android:textColor="@color/icons"
            android:text="@string/cbpesados" />

        <CheckBox
            android:id="@+id/cbjeta"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:textSize="16sp"
            android:textColor="@color/icons"
            android:text="@string/cbjeta" />
    </LinearLayout>

    </LinearLayout>





</FrameLayout>
